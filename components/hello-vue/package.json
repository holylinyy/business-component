{
	"name": "@hlstarter/hello-vue",
	"version": "1.0.0",
	"private": true,
	"main": "dist/v3/index.js",
	"exports": {
		".": {
			"types": "./dist/v3/index.d.ts",
			"import": "./dist/v3/index.es.js",
			"require": "./dist/v3/index.cjs.js"
		}
	},
	"types": "dist/v3/index.d.ts",
	"scripts": {
		"dev": "vite build --watch",
		"build": "w-build",
		"lint": "eslint --fix",
		"runBuild": "pnpm run build:2 && pnpm run build:27 && pnpm run build:3",
		"build:2": "vue-demi-switch 2 vue2 && vite build --config ./build/vite2.config.ts",
		"build:27": "vue-demi-switch 2.7 vue2.7 && vite build --config ./build/vite27.config.ts",
		"build:3": "vue-demi-switch 3 vue3 && vite build --config ./build/vite3.config.ts",
		"postinstall": "node -e \"try{require('./scripts/postinstall.js')}catch(e){console.log(e)}\""
	},
	"dependencies": {
		"vue-demi": "^0.14.7"
	},
	"devDependencies": {
		"@hlstarter/build": "workspace:^"
	},
	"peerDependencies": {
		"@vue/composition-api": "^1.7.2",
		"vue": ">=2.6.14"
	},
	"peerDependenciesMeta": {
		"@vue/composition-api": {
			"optional": true
		}
	},
	"module": "dist/v3/index.mjs",
	"style": "dist/v3/style.css"
}
